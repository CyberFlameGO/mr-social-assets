{
  "name": "MeetingRoom",
  "version": "0.0.1",
  "meta": {
    "authors": [
      {
        "name": "Mozilla"
      }
    ],
    "images": [
      {
        "type": "preview-thumbnail",
        "srcset": "./MeetingRoomScreen.jpg"
      }
    ]
  },
  "output": {
    "filePath": "rooms/meetingroom"
  },
  "assets": [
    {
      "name": "MeetingroomMesh",
      "src": "./MeetingSpaceMeshes.fbx",
      "gltf-unlit-generator": {
        "skip": true
      },
      "FBX2glTF": {
        "args": ["--khr-materials-unlit"]
      },
      "components": [
        {
          "nodes": {
            "CliffVista_mesh": {
              "xr": {
                "ar": false
              },
              "layers": {
                "reflection": true
              }
            },
            "FloorNav_mesh": {
              "visible": false,
              "nav-mesh": ""
            },
            "Water": {
              "water": {
                "forceMobile": true
              },
              "xr": {
                "ar": false
              }
            },
            "Skybox_node": {
              "skybox": {
                "azimuth": 0.28,
                "inclination": 0.44
              },
              "light": {
                "type": "ambient",
                "color": "#FFF"
              },
              "layers": {
                "reflection": true
              },
              "xr": {
                "ar": false
              }
            },
            "Spawner": {
              "gltf-model-plus": {
                "src": "#interactable-duck"
              },
              "css-class": "interactable",
              "super-spawner": {
                "template": "#interactable-template"
              },
              "body": {
                "mass": 0,
                "type": "static",
                "shape": "box"
              },
              "collision-filter": {
                "collisionForces": false
              },
              "quack": "",
              "sound": [
                {
                  "src": "#quack",
                  "on": "quack",
                  "poolSize": 2
                },
                {
                  "src": "#specialquack",
                  "on": "specialquack"
                }
              ]
            },
            "DirectionalLight_node": {
              "hide-when-quality": "low",
              "light": {
                "type": "directional",
                "color": "#F9FFCE",
                "intensity": 0.6
              }
            },
            "SpawnPoint1": {
              "spawn-point": ""
            }
          }
        },
        "./MeetingSpace_collision.json"
      ]
    }
  ]
}
