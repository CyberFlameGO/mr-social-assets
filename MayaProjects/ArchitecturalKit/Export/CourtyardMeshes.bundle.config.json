{
  "name": "Courtyard",
  "version": "0.0.1",
  "meta": {
    "authors": [
      {
        "name": "Mozilla"
      }
    ]
  },
  "output": {
    "filePath": "rooms/courtyard"
  },
  "assets": [
    {
      "name": "CourtyardMeshes",
      "src": "./CourtyardMeshes.fbx",
      "components": [
        {
          "nodes": {
            "NavMesh": {
              "visible": false,
              "nav-mesh": ""
