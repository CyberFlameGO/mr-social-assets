            "Skybox_node": {
              "skybox": {
                "azimuth": 0.28,
                "inclination": 0.44
              },
              "light": {
                "type": "ambient",
                "color": "#FFF"
              }
            },
            "DirectionalLight_node": {
              "light": {
                "type": "directional",
                "color": "#F9FFCE",
                "intensity": 0.6
              },
              "hide-when-quality": "low"
            }
          }
        }
      ],
      "gltf-unlit-generator": {
        "lighten": 0.2
      }
    }
  ]
}
